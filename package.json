{
  "name": "aileader",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter @aileader/api run dev\" \"pnpm --filter @aileader/web run dev\"",
    "build": "pnpm --filter @aileader/web run build",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\" --ignore-path .gitignore",
    "test": "npm test -w apps/api"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/swagger-ui": "^5.2.5",
    "fastify": "^5.7.4"
  }
}
